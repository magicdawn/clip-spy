{
  "name": "clip-spy",
  "version": "0.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "clip-spy",
    "package": {
      "name": "@magicdawn/clip-spy"
    },
    "triples": {
      "additional": [
        "aarch64-apple-darwin"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.4.0",
    "@types/node": "^17.0.8",
    "@swc/core": "^1.2.129",
    "@swc/helpers": "^0.3.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "rm -rf lib; rm tsconfig.tsbuildinfo; tsc",
    "build:debug": "napi build --platform --js addon.js --dts addon.d.ts",
    "prepublishOnly": "npm run build",
    "version": "napi version",
    "start": "ts-node ./src/index.ts",
    "start:debug": "node --inspect-brk -r ts-node/register ./src/index.ts"
  }
